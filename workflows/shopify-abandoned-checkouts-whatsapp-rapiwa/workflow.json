    {
      "parameters": {
        "jsCode": "// ====== CONFIG (Lean) ======\n// ENV vars recomendadas:\n// SHOPIFY_STORE=\"tu-tienda.myshopify.com\"\n// ABANDON_MIN_AGE_MIN=\"30\"   (minutos)\n// ABANDON_MAX_AGE_H=\"48\"     (horas)\n// DEFAULT_COUNTRY_CODE=\"54\"  (AR) / \"52\"(MX) / \"57\"(CO) etc.\n//\n// Shopify Access Token: credential en nodo \"Shopify: Fetch Abandoned Checkouts\" (header X-Shopify-Access-Token).\n\nconst shop = ($env.SHOPIFY_STORE || \"REPLACE_ME.myshopify.com\").trim();\nconst minAgeMin = parseInt($env.ABANDON_MIN_AGE_MIN || \"30\", 10);\nconst maxAgeH  = parseInt($env.ABANDON_MAX_AGE_H   || \"48\", 10);\n\nconst now = new Date();\nconst created_at_max = new Date(now.getTime() - minAgeMin * 60 * 1000); // antes de esto\nconst created_at_min = new Date(now.getTime() - maxAgeH * 60 * 60 * 1000); // no mÃ¡s viejo que esto\n\nreturn [{\n  json: {\n    shop,\n    created_at_min: created_at_min.toISOString(),\n    created_at_max: created_at_max.toISOString(),\n    run_at: now.toISOString(),\n  }\n}];\n"
      },
